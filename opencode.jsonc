{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "magic": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@21st-dev/magic@latest"
      ],
      "environment": {
        "MAGIC_API_KEY": "YOUR_MAGIC_API_KEY"
      },
      "enabled": false
    },
    "supabase-kyrie": {
      "type": "local",
      "command": [
        "node",
        "C:\\Users\\lukka\\AppData\\Roaming\\npm\\node_modules\\@supabase\\mcp-server-supabase\\dist\\transports\\stdio.js"
      ],
      "environment": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY",
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      },
      "enabled": false
    },
    "supabase-prod": {
      "type": "local",
      "command": [
        "node",
        "C:\\Users\\lukka\\AppData\\Roaming\\npm\\node_modules\\@supabase\\mcp-server-supabase\\dist\\transports\\stdio.js"
      ],
      "environment": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY",
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      },
      "enabled": false
    },
    "supabase-distribuidora": {
      "type": "local",
      "command": [
        "node",
        "C:\\Users\\lukka\\AppData\\Roaming\\npm\\node_modules\\@supabase\\mcp-server-supabase\\dist\\transports\\stdio.js"
      ],
      "environment": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY",
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      },
      "enabled": true
    },
    "supabase-dev": {
      "type": "local",
      "command": [
        "node",
        "C:\\Users\\lukka\\AppData\\Roaming\\npm\\node_modules\\@supabase\\mcp-server-supabase\\dist\\transports\\stdio.js"
      ],
      "environment": {
        "SUPABASE_URL": "YOUR_SUPABASE_URL",
        "SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY",
        "SUPABASE_SERVICE_ROLE_KEY": "YOUR_SUPABASE_SERVICE_ROLE_KEY",
        "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
      },
      "enabled": false
    },
    "context7": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@upstash/context7-mcp",
        "--api-key",
        "YOUR_CONTEXT7_KEY"
      ],
      "enabled": true
    },
    "filesystem": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "c:/",
        "d:/"
      ],
      "enabled": true
    },
    "sequential-thinking": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "enabled": false
    },
    "supabase-mcp-server": {
      "type": "local",
      "command": [
        "node",
        "C:\\Users\\lukka\\AppData\\Roaming\\npm\\node_modules\\@supabase\\mcp-server-supabase\\dist\\transports\\stdio.js",
        "--access-token",
        "YOUR_ACCESS_TOKEN"
      ],
      "enabled": false
    },
    "shadcn": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "mcp-remote",
        "https://www.shadcn.io/api/mcp"
      ],
      "enabled": false
    },
    "heroui-react": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@heroui/react-mcp@latest"
      ],
      "enabled": false
    },
    "heroui": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@heroui/mcp"
      ],
      "enabled": false
    },
    "trello": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@delorenj/mcp-server-trello"
      ],
      "environment": {
        "TRELLO_API_KEY": "YOUR_TRELLO_API_KEY",
        "TRELLO_TOKEN": "YOUR_TRELLO_TOKEN"
      },
      "enabled": false
    },
    "stitch": {
      "type": "remote",
      "url": "https://stitch.googleapis.com/mcp",
      "headers": {
        "X-Goog-Api-Key": "YOUR_GOOGLE_API_KEY"
      },
      "enabled": false
    }
  }
}